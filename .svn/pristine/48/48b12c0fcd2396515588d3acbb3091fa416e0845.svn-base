package kr.or.navi.board.photo;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("/board/photo")
public class photoController {

	@GetMapping
	public String photoPage() {
		return "board/photo";
	}
	
	@GetMapping("register")
	public String photoRegisterPage() {
		return "board/photo_register";
	}

	@GetMapping("detail")
	public String photoDetailPage() {
		return "board/photo_detail";
	}

	@GetMapping("modify")
	public String photoModifyPage() {
		return "board/photo_mod";
	}
}
