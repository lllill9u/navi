@charset "UTF-8";
@import url("../../font/GmarketSans/GmarketSans.css");
@import url("../../font/NotoSansKR/NotoSansKR.css"); 
@import url("../../font/CaviarDreams/CaviarDreams.css");
@import url("../common/common_icon.css");
@import url("../common/common_table.css");
@import url("../common/calendar.css");

*{font-family:'NotoSansKR',sans-serif;line-height:1.3;box-sizing:border-box!important}
html,body{width:100%;height:100%;overflow-x:hidden;position:relative}
em,i{font-style:normal}
a{display:inline-block;text-decoration:none;color:#333;transition:.3s}
a:hover,a:focus{
    font-weight: normal;
}
ul{padding-left:0;margin: 0;}
li{list-style:none}
.skip,.sr-only{display:block;font-size:0;clip:rect(0,0,0,0);margin:-1px;padding:0;position:absolute}
.prog{max-width:1600px; width: 100%; margin: 0 auto;}
.btn-custom{color:#fff;background-color:#5346DB;border-color:#5346DB}
.btn-custom:hover,.btn-custom:focus{background-color:#333840;border-color:#333840;color:#fff}
#progMenu{position:fixed;top:0;left:10px;z-index: 10;}
#progMenu .ul{display:block;height:0;opacity:0;visibility:hidden;transition:all .3s}
#progMenu .ul.open{display:block;height:640px;opacity:1;visibility:visible;background-color: #fff; border:1px solid #ddd; padding:30px; overflow-y: auto;}
#progMenu .li{display:block}
.icon{display:inline-block;line-height:1;margin-right:5px;background-position:center center;background-repeat:no-repeat;width:24px;height:24px}

[class^="-box"]{border-radius:10px}
/* [class^=tit]{font-weight:700;display:inline-block} */
h1{border-bottom:1px dashed #ececec;position:relative;font-family:'GmarketSans'}
.version{position:absolute;right:0;bottom:10px}
select+input[type="text"]{margin:0 2px 0 3px}
input[readonly]{cursor:default;background-color:#ececec}
input[readonly]:focus{border-color:#8888;box-shadow:none;background-color:#ececec}
.btn{vertical-align:top}
.flex,.form-inline{display:inline-flex;align-items:center}
li{min-height:25px}
.loginUser{position:relative;bottom:-5px}
.searchBox{padding:20px;border-radius: 5px; background-color: #f8f8f8; }
.searchBox .btn{/* margin:0 2px; */}
.img-in{width:100%;height:100%;position:relative}
.img-in img{display:inline-block;width:auto;height:100%;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%)}
[data-pg-role="searchUI"]{height:80px}
[data-pg-role="searchUI"] *{border-radius:4px;height:100%}
.content{padding:20px;margin:20px 0;border:1px solid #ddd;border-radius:5px;position:relative}
.content .inner{overflow-y:auto;overflow-x:hidden}
.content .inner.code{margin:50px 0 0;border:1px solid #ddd;border-radius:4px}
.content pre{height:200px;overflow:hidden;overflow-y:auto;margin:0}
.tit.con-tit{font-size:18px;position:absolute;left:20px;top:20x}
.content ::-webkit-scrollbar{width:13px}
.content ::-webkit-scrollbar-track{background:rgba(24,119,242,0.1)}
.content ::-webkit-scrollbar-thumb{background:rgba(14,108,246,0.5);border-radius:7px;display:block}
.content ::-webkit-scrollbar-thumb:hover{background:rgba(0,0,0,0.5)}
.modal{display:none}
.modal-dialog{max-width:600px;width:100%;}
.modal-content{border-radius:9px;overflow:hidden}
.modal.show{background:rgba(0,0,0,0.7);opacity: 1;display: flex !important;align-items: center;justify-content: center;}
.btn.copy{position:absolute;right:15px;top:15px}
.login{border-radius:4px;padding:30px;background-color:rgba(13,110,253,0.1);max-width:500px;width:100%;margin:0 auto;text-align:center}
.login.tit{display:block}
.info-box{border:2px solid #fff}
.comment .register{margin-left:5px;min-width:80px}
.comment .card-body{padding :1rem 2rem}
.comment .comment-tit{font-size:16px;   
    margin: 20px 0;
    font-weight: bold;}
.comment .ul{}
.comment .ul,.comment .ul li{padding:0 !important;}
.comment .ul li:before{display:none !important}
.comment .comment-content{display:flex;align-items:flex-start;padding:10px 0 20px}
.comment .comment-content>*{display:inline-block;height:57px}
.comment .imgbox{width:50px;min-width:50px;height:57px}
.comment .writebox{width:calc(100% - 60px);margin-left:20px}
.comment .writebox span{display:block;display:-webkit-box;width:100%;height:100%;overflow:hidden;text-overflow:ellipsis;word-break:break-all;word-wrap:break-word;-webkit-line-clamp:3;-webkit-box-orient:vertical;line-height:1.2}
.comment .btn-close{width:27px}
.comment .comment-info{padding:0 0 20px}
.comment .comment-info span{display:inline-flex;align-items:center}
.comment .ul.depth2 li{padding-left:75px}
.customUl{padding-left:15px}
.customUl li{position:relative;padding-left:100px;margin:10px 0}
.customUl li:after{content:'';display:block;clear:both}
.customUl li > .tit:first-child{position:absolute;left:0;text-align:justify;font-size:inherit;margin:0}
.customUl li > .tit:before{content:'';display:inline-block;width:4px;height:4px;background-color:#0d6efd;position:relative;top:-4px;margin-right:4px;border-radius:50%}
.box-inner{padding:50px;text-align:center}
.mypage .imgbox{width:100%;max-width:200px;height:200px;border-radius:10px;overflow:hidden;margin:0 auto 20px;background-color:#B6AEF4;padding:20px 10px 0}
.mypage-login .input-info{max-width:200px;width:100%;margin:0 auto}
.mypage-login .btn-login{width:100%}
.category{display:inline-block;color:#fff;padding:7px 10px;font-size:15px;border-radius:4px;line-height:1}
.category.project,
.category.permission,
.category.new{background-color:#5346DB;margin-right:5px}
.category.share{background-color:#0d6efd;}
.category.disable{background-color: #6c757d;}
.category.emergency,
.category.deny{color: #E92C2C;background-color: #FFECEC;}
.category.ordinary{color:#585757;background-color: #E8E8E8;}
.category.all{color:#333;background-color: #fff;}

/* view */
.view{border-top:3px solid #5346DB}
.view_inner > div{padding:20px 30px}
.view-info{background-color:#E3E4FA}
.view-tit{border-bottom:2px solid #fff;padding-bottom:20px;display:block;margin:0 0 20px; position: relative;}
.view-tit em{display:inline-block;vertical-align:middle; font-size: 25px;}
.view-tit.icon-tit{padding-left: 30px}
.view-tit.icon-tit .icon{position: absolute;left:0;top:5px}
.view-tit.icon-tit + ul{margin-bottom: 0;}

.view .ul{padding-left:0}
.view .ul li{float:left;min-width:50%;position:relative;padding-left:100px}
.view .ul li:before{display:inline-block;content:'';width:5px;height:5px;background-color:#5346DB;border-radius:50%;position:absolute;top:50%;left:0;margin-right:10px;transform: translateY(-50%);}
.view .list-tit{font-size:15px;position:absolute;left:10px;top:50%;transform: translateY(-50%);margin:0; font-weight: bold;}
.view_content{min-height:200px;border-bottom:1px solid #ddd}

/* viewEdit */
.view.edit .view_inner > [class*="cke_editor_viewEdit"]{padding:  0;}
.ck-editor__main{height:500px;overflow-y:auto}
.ck-content{height:100%}

/* commonform */
.commonForm{border-top:3px solid #5346DB;max-width:1600px}
.commonForm legend{margin-bottom:0}
.commonForm .row{border-bottom:1px solid #ddd;padding:10px 0}
.commonForm .row > div{display:flex;align-items:center}
.commonForm .label{border-right:1px solid #ddd}
.commonForm .form-label{margin:0}
.commonForm .form_content .input{width:100%}
.commonForm .text .form_content .form-control{max-width:330px}
.commonForm .tel .input{max-width:100px}
.commonForm .identifyNum .input{max-width:42.5%}
.commonForm .email .input:first-of-type{max-width:15%}
.commonForm .email .input:nth-of-type(2){max-width:20%}
.commonForm .email .input:last-of-type{max-width:20%}
.commonForm .entity{margin:0 4px}
.commonForm .address .btn{white-space:nowrap}
.commonForm .address .form_content{display:block}
.commonForm .address .input{max-width:600px}
.commonForm .select .input{max-width:20%}
.row.profil > .form_content{display:block}
.profil_img,.profil .filebtn{width:100px}
.profil_img{height:100px;margin:0 0 5px}
.profil .file{position:relative;height:38px}
.profil .file *{height:100%;width:100px}
.profil .fileInput{position:absolute;z-index:2;top:0;left:0;opacity:0}

/* table */
.table .category{font-size: 15px;}

/* calendar */
.cal-today_list{margin-bottom: 0; flex-wrap:wrap}
.cal-today_list li{margin-right: 2rem; min-width: 98px;}
.cal-today_box{background:#E7F3FF;
    padding: 15px 10px;
    border-radius: 9px;}

.cal-today_box *{letter-spacing:-2px;}
.cal-today_box.cal-modal{
    padding:20px;
}

.cal-Modal_table{overflow:scroll;max-width: 100%;width:100%; min-height: 200px; max-height: 420px;}

.cal-today_box .category{font-weight: bold; font-size: 15px; margin-right: 15px;}
.cal-Modal_table .table,.cal-Modal_table .pageBox .pagination{width: 1200px;}

/* photolist */ 
.photo_list{}
.photo_list .card{min-height: 400px; max-height: 400px; height: 100%; position:relative}
.photo_list .photo_img{height: 300px; width: 100%; background-color: #B6AEF4; position: relative; overflow: hidden;}
.photo_list .photo_img:after{content: ''; display:block;width: 100%; height: 100%; background: rgba(255,255,255,0.5); position: absolute; top:0;left: 0;}
.photo_list .card-body{}
.photo_list .card-title{ display:block;padding:5px 0 20px;border-bottom: 1px dashed #ddd;}
.photo_list .card-title em{
    display:block;
    white-space: normal;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-break: break-all;
    height: 100%;
    max-height: 40px;
    line-height: 1.2;
}
.photo_list button,.photo_list .card-footer{background: transparent;}
.photo_list .kinBtn{position: absolute; top:10px;right:0px}
.photo_list .card-footer{display:flex;justify-content: space-between; align-items: center; flex-wrap: wrap;}
.photo_list li{display:inline-block; min-width:90px;}
.photo_list li *{padding:0;display:flex; align-items: center; justify-content: flex-start; white-space: nowrap;}
.photo_list li span{}

.photo-view_img{padding:20px 0 20px;margin-top: 20px;border-top: 1px solid #ddd;}
.photo-view_img .scroll_x{    overflow-x: scroll;
    max-width: 1600px;
    width: 100%;}
.photo-view_img ul{display:flex; margin: 0;}
.photo-view_img .photo-img_item{width: 200px;height: 200px;min-width: 200px;}

/* progressbar */
.progress{max-width: 100px; width: 100%;}

/* 다운로드파일 링크 */
.download_link .icon{height: 37px;width: 37px;}

/* 첨부파일목록 */
.addfile{}
.fileList > li{border-bottom:1px solid #ddd;padding: 6px 0;}
.fileList > li:first-child{border-top: 1px solid #ddd;}
.file_item > .icon{width: 37px;height: 37px;}
.file_name{font-size: 16px; padding-left:4px}


